# FlareSolverr Configuration Example

# Standard FlareSolverr settings
LOG_LEVEL=info
HOST=0.0.0.0
PORT=8191
HEADLESS=true

# Automatic Session Management (optional)
# AUTO_SESSION_MANAGEMENT=true
# SESSION_PER_DOMAIN=true
# AUTO_SESSION_TTL=30
# MAX_AUTO_SESSIONS=10

# Optional: Default proxy for all requests
# PROXY_URL=http://proxy.example.com:8080

# Optional: Captcha solver timeout
CAPTCHA_SOLVER=none

# Optional: Browser settings
BROWSER_TIMEOUT=40000